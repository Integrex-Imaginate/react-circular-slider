    "radial",
    "slider",
    "react hooks"
  ],
  "author": "Fidel Seehawer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fseehawer/react-circular-slider/issues"
  },
  "homepage": "https://fseehawer.github.io/react-circular-slider/",
  "peerDependencies": {
    "global": "^4.4.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^7.1.2",
    "eslint-utils": "^1.4.1",
    "global": "^4.4.0",
    "prop-types": "^15.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^3.0.1",
    "serialize-javascript": "^3.1.0",
    "vite": "^6.2.2",
    "vite-plugin-svgr": "^4.3.0",
    "webpack-cli": "^6.0.1"
