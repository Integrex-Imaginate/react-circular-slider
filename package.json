  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-demo": "vite build --config vite.demo.config.js",
    "gh-pages": "git subtree push --prefix build origin gh-pages",
    "build-publish": "npm run build && npm run build-demo && npm run gh-pages && npm publish --access public"
